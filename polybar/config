[bar/mybar]
modules-right = date
modules-left = battery

[module/battery]
type = internal/battery

label-discharging = Battery (d): %percentage%%
label-charging = Battery (c): %percentage%%

full-at = 99

battery = BAT0
adapter = ADP1

poll-interval = 5


[module/date]
type = internal/date
label = %date% %time%

date = %Y-%m-%d%
time = %H:%M:%S

interval = 1.0
